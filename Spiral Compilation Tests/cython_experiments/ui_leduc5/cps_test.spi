inl main () =
    inl p1 = agent.neural.create(fun game_state p p'_prob dist action => nodes.state p)
    inl p2 = agent.uniform.create()
    loop.for' (from:0u32 nearTo:100) (fun i =>
        open nodes
        inl r = leduc.game (nodes.cps.nodes_2p (p1.funs, p2.funs)) ((p1.init,p2.init),dyn id)
        $"print(\"Reward for player one at iteration \", !i, \" is \", !r)" : ()
        )