inl rec create forall t. size =
    typecase t with
    | ~a * ~b => create `a size, create `b size
    | ~t => !!!!ArrayCreate(`t, size)

inl rec index ar i = 
    match ar with
    | a,b => index a i, index b i
    | ar => !!!!ArrayIndex(ar,i)

inl rec set ar i v =
    match ar, v with
    | (a,b), (va,vb) => set a i va . set b i vb
    | _ => !!!!ArrayIndexSet(ar,i,v)