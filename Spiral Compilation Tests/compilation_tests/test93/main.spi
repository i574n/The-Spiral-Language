// Do the GADTs work?

union t y =
    | A :: t int
    | B :: t float
    | C :: string -> t string

let f forall y. (x : t y) =
    match x with
    | A =>
        inl x = x
        ()
    | B =>
        inl x = x
        inl y = x
        inl q = y
        ()
    | C sd => 
        inl x = x
        ()

inl main() : () = 
    inl x = f A
    inl x = f B
    inl x = f (C "qwe")
    ()