// Do the GADTs work?

union t y =
    | A :: t int
    | B :: t float
    | C :: t string

inl f forall y. (x : t y) =
    match x with
    | A =>
        inl x = x
        ()
    | B =>
        inl x = x
        inl y = x
        inl q = y
        ()
    | C => 
        inl x = x
        ()
