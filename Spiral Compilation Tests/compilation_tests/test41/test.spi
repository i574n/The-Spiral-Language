// Do the nested patterns work?
inl main() : i32 =
    match dyn ((1,2) :: (4,5) :: (5,6) :: []) : _ (i32 * i32) with
    | [] => "No elements", 0
    | (q,w) :: [] => "One element.", q+w
    | (q,w) :: (e,r) :: [] => "Two elements.",q+w+e+r
    | (q,w) :: (e,r) :: (t,y) :: l => "At least three elements.",q+w+e+r+t+y
    |> fun (a,b) => $"printf(\"%s\\n%i\\n\",!a->ptr,!b)"