type card = { suit : $"ap_uint<2>"; rank : $"ap_uint<4>" }
prototype qwe x : forall t. x -> t

type i_array (ar : * -> *) el = {
    merge : ar el -> ar el -> ar el
    }

inl main() = 
    global "#include <array>"
