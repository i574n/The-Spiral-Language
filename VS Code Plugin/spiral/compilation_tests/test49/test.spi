// Does structural comparison for union types work?
union t = A: i32 | B: f64
inl union_test () =
    inl a = A: 1
    inl b = B: 3
    $"// Static, Static"
    inl _ = dyn (comp a b)
    $"// Dyn, Static"
    inl _ = comp (dyn a) b
    $"// Static, Dyn"
    inl _ = comp a (dyn b)
    $"// Dyn, Dyn"
    inl _ = comp (dyn a) (dyn b)
    ()

open list
inl union_rec_test () =
    inl a = 1i32 :: 2 :: dyn (3 :: Nil)
    inl b = 1 :: dyn (2 :: 3 :: Nil)
    inl _ = comp a b
    ()

inl main () =
    $"// Union test"
    union_test ()
    $"// Union rec test"
    union_rec_test ()